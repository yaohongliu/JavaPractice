package Example1;

public class product {
	String name;
	double price;
	final static float discount = 5.5f;
	
	
	public static void main(String[] args) {
		product p1 = new product();
		p1.name = "Television";
		p1.price = 67.889;
		
	}
	public static peintProduct() {
		System.out.println("product name"+name);
		System.out.println("product price"_price)
		System.out.println("discount "+discount);;
	}

}
